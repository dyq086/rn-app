@import '../common/index.wxss';.van-tabbar {
  display: flex;
  width: 100%;
  height: 100px;
  height: var(--tabbar-height, 100px);
  background-color: #fff;
  background-color: var(--tabbar-background-color, #fff);
}
.van-tabbar--fixed {
  position: fixed;
  bottom: 0;
  left: 0;
}
.van-tabbar--safe {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
